@model List<fns.Models.Admin.VModels.vNews>

@foreach (var item in Model)
{
    <tr>
        <td>@item.id</td>
        <td>@item.cName</td>
        <td>@item.title</td>
        <td>@item.auth</td>
        <td>@item.insDt</td>
        <td style="display:flex;">
            <a href="javascript:void(0);" onclick="gotoEditNews(@item.id)"><i class="fa fa-pencil fa-fw"></i></a>
            <a href="javascript:void(0);" onclick="removeNews(@item.id)"><i class="fa fa-remove fa-fw"></i></a>
        </td>
    </tr>
}